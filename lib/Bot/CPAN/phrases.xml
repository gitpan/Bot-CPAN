<?xml version="1.0"?>
<!DOCTYPE phrasebook [
	<!ELEMENT phrasebook (dictionary)*>              
	<!ELEMENT dictionary (phrase)*>
	<!ATTLIST dictionary name CDATA #REQUIRED>
	<!ELEMENT phrase (#PCDATA)>
	<!ATTLIST phrase name CDATA #REQUIRED>
]>
<phrasebook>
	<dictionary name="EN">
		<phrase name="NO_DISTRIBUTION">
			No such distribution: $distribution
		</phrase>
		<phrase name="NO_MODULE">
			No such module: $module
		</phrase>
		<phrase name="NO_AUTHOR">
			No such author: $author
		</phrase>
		<phrase name="UNKNOWN">
            Unknown
		</phrase>
		<phrase name="COMMAND_FAILURE">
            A command failure occured :(
		</phrase>
		<phrase name="Bot::CPAN::status">
			$requests request$s since I started up at $start_time
		</phrase>
		<phrase name="CPANRATINGS_DOWN">
			cpanratings.perl.org is unavailable
		</phrase>
		<phrase name="Bot::CPAN::dist_ratings(no_ratings)">
			No ratings for: $module
		</phrase>
		<phrase name="Bot::CPAN::dist_ratings(ratings)">
			Recent ratings: $ratings; Overall (n=$n): mean $mean, median $median, min $min, max $max, stddev $stddev, mode $mode
		</phrase>
		<phrase name="Bot::CPAN::botsnack">
            &lt;3
		</phrase>
		<phrase name="Bot::CPAN::config(adminhost)">
			Adminhost: $adminhost
